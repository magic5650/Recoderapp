                <item name="android:colorPrimaryDark">@color/colorPrimaryDark</item>
                <item name="android:colorAccent">@color/accent_material_dark</item>
                <item name="android:colorBackground">@color/background_floating_material_light
                </item>
                <item name="android:colorForeground">@color/dim_foreground_material_dark</item>
                <item name="android:textColorPrimary">@color/abc_primary_text_material_light</item>
                <item name="android:textColorSecondary">
